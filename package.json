{
  "name": "gmork.in",
  "version": "0.0.1",
  "description": "gmork.in website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "babel-node server/index.js",
    "clean": "rm -rf static/js static/css && mkdir static/js static/css",
    "build": "npm-run-all clean -p build:js build:css",
    "build:js": "browserify src/js/index.js -o static/js/index.js",
    "build:css": "node-sass -r src/css/index.scss static/css/index.css",
    "watch": "npm-run-all clean build -p watch:js watch:css serve",
    "watch:js": "watchify src/js/index.js -o static/js/index.js -v -d",
    "watch:css": "node-sass -w -r src/css/index.scss static/css/index.css"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "amagitakayosi",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.8.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "remark": "^4.2.2",
    "remark-html": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "node-sass": "^3.7.0",
    "npm-run-all": "^1.8.0",
    "watchify": "^3.7.0"
  }
}
